{
  "name": "dynamic-navigator",
  "displayName": "Dynamic Navigator",
  "publisher": "SamuelFriedman",
  "repository": {
    "type": "git",
    "url": "https://github.com/sjfriedman/dynamic-navigator.git"
  },
  "license": "MIT",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.102.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:python",
    "onCommand:dynamicNavigator.gotoOrigin",
    "onCommand:dynamicNavigator.peekOrigin"
  ],
  "contributes": {
    "commands": [
      {
        "command": "dynamicNavigator.gotoOrigin",
        "title": "Dynamic Navigator: Go to Origin"
      },
      {
        "command": "dynamicNavigator.peekOrigin",
        "title": "Dynamic Navigator: Peek Origin"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorLangId == python",
          "command": "dynamicNavigator.gotoOrigin",
          "group": "navigation"
        },
        {
          "when": "editorLangId == python",
          "command": "dynamicNavigator.peekOrigin",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/vscode": "^1.102.0",
    "typescript": "^4.4.3"
  }
}
